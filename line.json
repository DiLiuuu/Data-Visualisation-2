{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "Global Missing Migrants Dataset.csv"
    },
    "mark": "line",
    "encoding": {
        "x": {
            "field": "Incident year",
            "timeUnit": "yearmonth",
            "type": "temporal",
            "title": "Time"
        },
        "y": {
            "field": "Total Number of Dead and Missing",
            "type": "quantitative",
            "title": "Fatalities"
        }
    }
}